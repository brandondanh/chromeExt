{
    "manifest_version": 2,
  
    "name": "WindowShopping",
    "description": "Obfuscates personal shopping data by randomly opening product pages.",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "EXTENSION"
    },
    "permissions": [
      "activeTab",
      "storage",
      "tabs"
    ],
    "background": {
        // "scripts": ["background.js"]
        "page": "background.html"
      }
  }