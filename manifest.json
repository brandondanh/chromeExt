{
    "manifest_version": 2,
  
    "name": "WindowShopper TestV2",
    "description": "Attempted using iframes",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "EXTENSION"
    },
    "permissions": [
      "activeTab",
      "storage",
      "tabs"
    ],

    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["jquery-3.2.1.js", "background.js"]
      }
    ]
    // "background": {
